ox\" value=\"mario\" v-model=\"names\">\r\n    <label >Mario</label>\r\n </div>\r\n\r\n\r\n \r\n<label >Skills:</label>\r\n<input type=\"text\" v-model=\"tempSkill\" @keyup=\"addSkill\">\r\n    <div v-for=\"skillin skills\" ></div>\r\n\r\n  </form>\r\n\r\n\r\n\r\n\r\n<!-- To show 2-way binding -->\r\n  <p>Email: {{ email }}</p>\r\n  <p>Password: {{ password }}</p>\r\n  <p>Role: {{ role }}</p>\r\n  <p>Terms accepted: {{ terms }}</p>\r\n  <p>Names: {{ names }}</p>\r\n\r\n\r\n \r\n</template>\r\n\r\n\r\n\r\n<script>\r\nexport default {\r\n    data(){\r\n        return{\r\n            email:'email address',\r\n            password:'*****',\r\n            role:'designer',\r\n            terms: false,\r\n            names: [],\r\n            tempSkill: '',\r\n            skills: []\r\n\r\n        }\r\n    },\r\n    methods:{\r\n       addSkill(e){\r\n        if(e.key === ',' && this.tempSkill){\r\n            this.skills.push(this.tempSkill)\r\n            this.tempSkill=''\r\n        }\r\n        \r\n       }\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\nform{\r\n    max-width: 420px;\r\n    margin: 30px auto;\r\n    background: white;\r\n    text-align: left;\r\n    padding: 40px;\r\n    border-radius: 10px;\r\n}\r\n\r\nlabel {\r\n    color: #aaa;\r\n    display: inline-block;\r\n    margin: 25px 0 15px;\r\n    font-size: 0.6em;\r\n    text-transform: uppercase;\r\n    letter-spacing: 1px;\r\n    font-weight: bold;\r\n}\r\n\r\ninput,select{\r\n    display: block;\r\n    padding: 10px 6px;\r\n    width: 100%;\r\n    box-sizing: border-box;\r\n    border: none;\r\n    border-bottom: 1px solid #ddd;\r\n    color: #555;\r\n}\r\n\r\ninput[type=\"checkbox\"]{\r\n    display: inline-block;\r\n    width: 16px;\r\n    margin: 0 10px 0 0;\r\n    position: relative;\r\n    top: 2px;\r\n}\r\n\r\n</style>"]},"metadata":{},"sourceType":"module"}